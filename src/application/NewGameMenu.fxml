<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NewGameController">
   <children>
      <VBox alignment="CENTER" prefHeight="800.0" prefWidth="1000.0">
         <children>
            <Button fx:id="play" mnemonicParsing="false" onAction="#playActionHandler" prefHeight="100.0" prefWidth="200.0" text="Play">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="chooseplayer" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Choose player">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="back" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Back" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
